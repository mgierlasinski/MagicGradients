<?xml version="1.0" encoding="utf-8" ?>
<resources>
	<declare-styleable name="GradientView">
		<attr name="gradientSource" format="string" />
		<attr name="gradientSize" format="string" />
		<attr name="gradientRepeat" format="enum">
			<enum name="repeat" value="0"/>
			<enum name="repeat_x" value="1"/>
			<enum name="repeat_y" value="2"/>
			<enum name="no_repeat" value="3"/>
		</attr>
	</declare-styleable>
</resources>

