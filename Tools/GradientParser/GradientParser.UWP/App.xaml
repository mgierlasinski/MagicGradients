<Application
    x:Class="GradientParser.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:converters="using:GradientParser.Converters">
    <Application.Resources>
        <converters:BooleanToVisibilityConverter x:Key="BooleanToVisibility" />
        <converters:InvertedBooleanToVisibilityConverter x:Key="InvertedBooleanToVisibility" />
        <converters:StatusToBooleanConverter x:Key="StatusToBoolean" />
    </Application.Resources>
</Application>
